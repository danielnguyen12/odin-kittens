<%= form_with model: @kitten do |f| %>
  <%= f.label :name, 'Name:' %>
  <%= f.text_field :name %>
  <%= f.label :age, 'Age:' %>
  <%= f.number_field :age %>
  <%= f.label :cuteness, 'Cuteness' %>
  <%= f.range_field :cuteness, in: 1..10 %>
  <%= f.label :softness, 'Softness' %>
  <%= f.range_field :softness, in: 1..10 %>
  <%= f.submit 'Submit' %>
<% end %>